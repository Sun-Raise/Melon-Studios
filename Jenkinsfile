
pipeline {
    agent any
        stage('CheckOut') {
			steps {
				echo 'Hi Anand, Welcome to automate pipeline'
			}
        }
        stage('Compile') {
			steps {
			    echo 'Compailing the code'
			}
        }
        stage('test') {
			steps {
			    echo 'Testing the code'
			}
        }
        stage('deploy') {
		    steps {
			    echo 'Deploying the code'
			}
        }
    }
}
